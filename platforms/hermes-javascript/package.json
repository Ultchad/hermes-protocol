{
  "name": "hermes-javascript",
  "version": "0.0.1",
  "description": "Hermes javascript bindings",
  "keywords": [
    "snips",
    "hermes",
    "mqtt",
    "voice"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/snipsco/hermes-protocol"
  },
  "bugs": {
    "url": "https://github.com/snipsco/hermes-protocol/issues"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "npm run lint && npm run test",
    "lint": "eslint src",
    "postinstall": "node scripts/postinstall.js",
    "build:sources": "node scripts/make.js",
    "test": "npm run test:mqtt && npm run test:roundtrip",
    "test:mqtt": "jest ./tests/mqtt/mqttTests.spec.js",
    "test:roundtrip": "jest ./tests/casts/roundtrips.spec.js"
  },
  "author": "Julien Elbaz",
  "license": "MIT",
  "files": [
    "src",
    "scripts"
  ],
  "dependencies": {
    "chalk": "^2.4.1",
    "ffi-napi": "^2.4.4",
    "node-int64": "^0.4.0",
    "ref": "^1.3.5",
    "ref-array": "^1.2.0",
    "ref-struct": "^1.1.0",
    "tmp": "0.0.33"
  },
  "devDependencies": {
    "camelcase": "^5.0.0",
    "eslint": "^5.9.0",
    "eslint-plugin-jest": "^22.1.0",
    "jest": "^23.6.0",
    "mqtt": "^2.18.8",
    "segfault-handler": "^1.0.1"
  }
