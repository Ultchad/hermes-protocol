{
    "env": {
        "es6": true,
        "node": true,
        "jest/globals": true
    },
    "parser": "typescript-eslint-parser",
    "plugins": ["jest", "typescript"],
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": "warn",
        "quotes": ["error", "single", { "avoidEscape": true }],
        "quote-props": ["error", "as-needed"],
        "typescript/no-unused-vars": "off"
    }
}